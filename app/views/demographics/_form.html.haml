= form_for @demographic do |f|
  - if @demographic.errors.any?
    #error_explanation
      %h2= "#{pluralize(@demographic.errors.count, "error")} prohibited this demographic from being saved:"
      %ul
        - @demographic.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :data
    = f.text_field :data
  .actions
    = f.submit 'Save'
